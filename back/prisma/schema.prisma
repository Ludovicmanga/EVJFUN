generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Destination {
  id                       Int     @id @default(autoincrement())
  name                     String  @unique
  in_france                Boolean
  access_to_mountain       Boolean
  access_to_lake           Boolean
  party                    Boolean
  historic                 Boolean
  wine                     Boolean
  first_privileged_season  String
  second_privileged_season String
}
